{
    "name": "liyuze/laravel-collection-macros",
    "description": "laravel collection macro",
    "keywords": [
        "laravel",
        "collection-macro",
        "macro"
    ],
    "homepage": "https://github.com/liyuze/laravel-collection-macros",
    "license": "MIT",
    "type": "laravel-package",
    "authors": [
        {
            "name": "liyuze",
            "email": "290315384@qq.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Liyuze\\CollectionMacros\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Liyuze\\CollectionMacros\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Liyuze\\CollectionMacros\\CollectionMacrosServiceProvider"
            ]
        }
    }
}
